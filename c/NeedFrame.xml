                    <Anchor point="CENTER">
                        <Offset>
                            <AbsDimension x="-20" y="-18"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        NeedFrame:ScaleWindow('down')
                    </OnClick>
                </Scripts>
            </Button>
            <Button name="$parentScaleUp" inherits="UIPanelButtonTemplate2" text="+">
                <Size>
                    <AbsDimension x="24" y="16"/>
                </Size>
                <Anchors>
                    <Anchor point="CENTER">
                        <Offset>
                            <AbsDimension x="50" y="-18"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        NeedFrame:ScaleWindow('up')
                    </OnClick>
                </Scripts>
            </Button>
        </Frames>

        <Scripts>
            <OnLoad>
                this:RegisterForDrag("LeftButton");
                this:SetMovable(1);
                this:SetUserPlaced(true);
            </OnLoad>
            <OnDragStart>
                this:StartMoving();
                this.isMoving = true;
            </OnDragStart>
            <OnDragStop>
                this:StopMovingOrSizing();
                this.isMoving = false;
            </OnDragStop>
        </Scripts>
    </Frame>

</Ui>
